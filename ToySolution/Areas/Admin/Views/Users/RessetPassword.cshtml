@model StoreUser
@{
    ViewData["Title"] = "RessetPassword";
}


<div class="row">
    <div class="col-12">
        <h1>Email: @Model.Email</h1>
        <h1>UserName: @Model.UserName</h1>
    </div>
    <div class="d-flex justify-content-end aligen-items-center">

        <form method="post">

            <input type="password" name="newPassword" value="Enter New Paword" />


            <input type="submit" class="btn btn-danger" value="Reset Password" />

        </form>

        <a asp-action="index" class="btn btn-dark ml-3">Go Back</a>
    </div>
</div>